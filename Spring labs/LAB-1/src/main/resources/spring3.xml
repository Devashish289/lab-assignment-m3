<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
		http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-4.3.xsd">
	<bean id="sbu" class="com.sunsoft.c.SBU">
	<property name="sbuCode" value="500"></property>
	<property name="sbuName" value="Product Engineering Services"></property>
	<property name="sbuHead" value="Narendra"></property>
	<property name="emplist" >
	<list>
	<bean class="com.sunsoft.c.Employee">
	<property name="employeeId" value="501"></property>
	<property name="employeeName" value="Devashish Singh"></property>
	<property name="salary" value="70000"></property>
	</bean>
	<bean class="com.sunsoft.c.Employee">
	<property name="employeeId" value="502"></property>
	<property name="employeeName" value="Abhijeet Singh"></property>
	<property name="salary" value="55000"></property>
	</bean>
	</list>
	</property>
	</bean>

</beans>
